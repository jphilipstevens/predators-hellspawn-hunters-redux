//WRISTBLADES

ACTOR Wristblade : Weapon replaces Fist
{
  Inventory.ForbiddenTo HeavyPredator
 +INVENTORY.RESTRICTABSOLUTELY
  Weapon.SlotNumber 1
  Weapon.SelectionOrder 3700
  +WEAPON.AXEBLOOD
  +WEAPON.NOALERT
  +INVENTORY.UNDROPPABLE
  +WEAPON.MELEEWEAPON
  States
  {
  Ready:
	TNT1 A 0 A_PlayWeaponSound("HCLAWSEL")
	WRST AB 2
	WRST CDEFGH 1
	WRST I 5
	WRST JKLM 1
    Goto RealReady
  RealReady:
  TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "ReadyZoom")
  PRCL A 1 A_WeaponReady(WRF_ALLOWZOOM)
	Loop
  Zoom:
    TNT1 A 0 A_JumpIfInventory("PredZoomLatch", 1, "ZoomLatchHold")
    TNT1 A 0 A_GiveInventory("PredZoomLatch", 1)
    TNT1 A 0 A_JumpIfInventory("PredZoom", 3, "ZoomOut")
    TNT1 A 0 A_JumpIfInventory("PredZoom", 2, "Zoom3")
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "Zoom2")
  Zoom1:
    TNT1 A 1 A_ZoomFactor(2.0)
    TNT1 A 0 A_GiveInventory("PredZoom", 1)
    TNT1 A 0 A_PlaySound("/predator/pzoomin")
    Goto ZoomCooldown
  Zoom2:
    TNT1 A 1 A_ZoomFactor(4.0)
    TNT1 A 0 A_GiveInventory("PredZoom", 1)
    TNT1 A 0 A_PlaySound("/predator/pzoomin")
    Goto ZoomCooldown
  Zoom3:
    TNT1 A 1 A_ZoomFactor(8.0)
    TNT1 A 0 A_GiveInventory("PredZoom", 1)
    TNT1 A 0 A_PlaySound("/predator/pzoomin")
    Goto ZoomCooldown
  ZoomOut:
    TNT1 A 1 A_ZoomFactor(1.0)
    TNT1 A 0 A_TakeInventory("PredZoom", 3)
    TNT1 A 0 A_PlaySound("/predator/pzoomout")
    Goto ZoomCooldown
  ZoomCooldown:
    TNT1 A 6
    TNT1 A 0 A_TakeInventory("PredZoomLatch", 1)
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "ReadyZoom")
    Goto RealReady
  ZoomLatchHold:
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "ReadyZoom")
    Goto RealReady
  ReadyZoom:
    PRCL A 1 A_WeaponReady(WRF_ALLOWZOOM)
    Loop
  Deselect:
    TNT1 A 0 A_TakeInventory("PredZoom", 3)
    TNT1 A 0 A_ZoomFactor(1.0)
    WRST A 1 A_Lower
	NULL AAA 0 A_Lower
    Loop
  Select:
    WRST A 1 A_Raise
	NULL AAA 0 A_Raise
    Loop
  Fire:
	TNT1 A 0 A_JumpifInventory("LightPredatorTag", 1, "LightPredFire")
	TNT1 A 0 A_JumpIfInventory("PowerStrength", 1, "BerserkedAttack")
	TNT1 A 0 A_JumpIfInventory("Poison", 1, "PoisonAttack")
	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PRAT GHI 1
    PRAT J 2 A_CustomPunch (random(2,3)*5,0,0,"BladePuff",96)
	PRAT IHGFEDC 1
	PRAT A 0 A_ReFire
    Goto RealReady
   BerserkedAttack:
	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PRAT GHI 1
    PRAT J 2 A_CustomPunch (random(10,15)*5,0,0,"BladePuff",96)
	PRAT IHGFEDC 1
	PRAT A 0 A_ReFire
    Goto RealReady
	PoisonAttack:
	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PRAT GHI 1
    PRAT J 2 A_CustomPunch (random(2,3)*5,0,0,"BladePuffPoison",96)
	PRAT IHGFEDC 1
	PRAT A 0 A_ReFire
    Goto RealReady
  AltFire:
  	TNT1 A 0 A_JumpIfInventory("PowerStrength", 1, "BerserkedAlt")
	TNT1 A 0 A_JumpIfInventory("Poison", 1, "PoisonAltAttack")
	TNT1 A 0 A_JumpIfInventory("LightPredatorTag", 1, "LightPredAlt")
	TNT1 A 0 A_Jump(128, "Alt2")
    PRAT LM 4
	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PRAT NOPQ 1
    PRAT R 2 A_CustomPunch (60,1,0,"BladePuff2",96)
	PRAT STUVWX 1
	PRAT A 0 A_ReFire
    Goto RealReady
  Alt2:
    PRAT BCDEF 2
	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PRAT GHIJ 1
    PRAT K 4 A_CustomPunch (60,1,0,"BladePuff2",96)
	PRAT IHGFEDC 1
	PRAT A 0 A_ReFire
    Goto RealReady
  PoisonAltAttack:
	TNT1 A 0 A_Jump(128, "PoisonAlt2")
    PRAT LM 4
	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PRAT NOPQ 1
    PRAT R 2 A_CustomPunch (60,1,0,"BladePuffPoison",96)
	PRAT STUVWX 1
	PRAT A 0 A_ReFire
    Goto RealReady
	PoisonAlt2:
    PRAT BCDEF 2
	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PRAT GHIJ 1
    PRAT K 4 A_CustomPunch (60,1,0,"BladePuffPoison",96)
	PRAT IHGFEDC 1
	PRAT A 0 A_ReFire
    Goto RealReady
	BerserkedAlt:
