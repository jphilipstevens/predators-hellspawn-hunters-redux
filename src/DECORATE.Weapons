    PRAT J 1 A_CustomPunch (random(2,3)*5,0,0,"BladePuff")
	PRAT IHGFE 1
	PRAT A 0 A_ReFire
    Goto RealReady
  PoisonLightPredAttack:
	TNT1 A 0 A_Jump(64, "CriticalHitPoison")
	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PRAT GHI 1
    PRAT J 1 A_CustomPunch (random(2,3)*5,0,0,"BladePuffPoison")
	PRAT IHGFE 1
	PRAT A 0 A_ReFire
    Goto RealReady
	BerserkedLPAttack:
	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PRAT GHI 1
    PRAT J 1 A_CustomPunch (random(10,15)*5,0,0,"BladePuff")
