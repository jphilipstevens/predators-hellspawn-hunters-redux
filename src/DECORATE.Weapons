    Goto ZoomCooldown
  Zoom3:
    TNT1 A 1 A_ZoomFactor(8.0)
    TNT1 A 0 A_GiveInventory("PredZoom", 1)
    TNT1 A 0 A_PlaySound("/predator/pzoomin")
    Goto ZoomCooldown
  ZoomOut:
    TNT1 A 1 A_ZoomFactor(1.0)
    TNT1 A 0 A_TakeInventory("PredZoom", 3)
    TNT1 A 0 A_PlaySound("/predator/pzoomout")
    Goto ZoomCooldown
  ZoomCooldown:
    TNT1 A 6
    TNT1 A 0 A_TakeInventory("PredZoomLatch", 1)
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "ReadyZoom")
    Goto Ready
  ZoomLatchHold:
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "ReadyZoom")
    Goto Ready
  ReadyZoom:
    PRSP A 1 A_WeaponReady(WRF_ALLOWZOOM)
    Loop
  Deselect:
    TNT1 A 0 A_TakeInventory("PredZoom", 3)
    TNT1 A 0 A_ZoomFactor(1.0)
    PRSP A 1 A_Lower
	NULL AAA 0 A_Lower
    Loop
  Select:
    PRSP A 1 A_Raise
	NULL AAA 0 A_Raise
    Loop
  Fire:
    TNT1 A 0 A_JumpIfInventory("PowerStrength", 1, "BerserkedLeftSwing")
    TNT1 A 0 A_JumpIfInventory ("RightSwing", 1, "RightSwing")
    LeftSwing:
    TNT1 A 0 A_GiveInventory ("Rightswing", 1)
    TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PSPA ABCD 1
    PSPA E 1 A_CustomPunch (80,1,0,"BladePuff",128)
    PSPA FGHIJ 1
	TNT1 A 1
    PRSP EDCB 1
    PRSP A 10 
	PRSP A 1 A_ReFire
