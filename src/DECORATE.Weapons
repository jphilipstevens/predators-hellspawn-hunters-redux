	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PRAT NOPQ 1
    PRAT R 2 A_CustomPunch (300,1,0,"BladePuff2",96)
	PRAT STUVWX 1
	PRAT A 0 A_ReFire
    Goto RealReady
   BerserkedAlt2:
    PRAT BCDEF 2
	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PRAT GHIJ 1
    PRAT K 4 A_CustomPunch (300,1,0,"BladePuff2",96)
	PRAT IHGFEDC 1
	PRAT A 0 A_ReFire
    Goto RealReady
  LightPredFire:
    TNT1 A 0 A_JumpIfInventory("PowerStrength", 1, "BerserkedLPAttack")
	TNT1 A 0 A_JumpIfInventory("Poison", 1, "PoisonLightPredAttack")
	TNT1 A 0 A_Jump(64, "CriticalHit")
