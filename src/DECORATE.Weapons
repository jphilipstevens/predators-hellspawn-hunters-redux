    TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PSPA KLMNO 1
    PSPA P 1 A_CustomPunch (80,1,0,"BladePuff",128)
    PSPA QRST 1
	TNT1 A 1
    PRSP EDCB 1
    PRSP A 10 
	PRSP A 1 A_ReFire
    Goto Ready
	Spawn:
    COMB A -1
    Stop
	BerserkedLeftSwing:
	TNT1 A 0 A_JumpIfInventory ("RightSwing", 1, "BerserkedRightSwing")
    TNT1 A 0 A_GiveInventory ("Rightswing", 1)
    TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PSPA ABCD 1
    PSPA E 1 A_CustomPunch (400,1,0,"BladePuff",128)
    PSPA FGHIJ 1
	TNT1 A 1
    PRSP EDCB 1
    PRSP A 10 
	PRSP A 1 A_ReFire
    Goto Ready
    BerserkedRightSwing:
    TNT1 A 0 A_TakeInventory ("Rightswing", 1)
    TNT1 A 0 A_PlayWeaponSound("PRMELEE")
	PSPA KLMNO 1
    PSPA P 1 A_CustomPunch (400,1,0,"BladePuff",128)
    PSPA QRST 1
	TNT1 A 1
    PRSP EDCB 1
    PRSP A 10 
	PRSP A 1 A_ReFire
    Goto Ready
	Spawn:
    COMB A -1
    Stop
