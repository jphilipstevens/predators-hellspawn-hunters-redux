    TNT1 A 0 A_JumpIfInventory("PredZoom", 2, "Zoom3")
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "Zoom2")
  Zoom1:
    TNT1 A 1 A_ZoomFactor(2.0)
    TNT1 A 0 A_GiveInventory("PredZoom", 1)
    TNT1 A 0 A_PlaySound("/predator/pzoomin")
    Goto ZoomCooldown
  Zoom2:
    TNT1 A 1 A_ZoomFactor(4.0)
    TNT1 A 0 A_GiveInventory("PredZoom", 1)
    TNT1 A 0 A_PlaySound("/predator/pzoomin")
    Goto ZoomCooldown
  Zoom3:
    TNT1 A 1 A_ZoomFactor(8.0)
    TNT1 A 0 A_GiveInventory("PredZoom", 1)
    TNT1 A 0 A_PlaySound("/predator/pzoomin")
    Goto ZoomCooldown
  ZoomOut:
    TNT1 A 1 A_ZoomFactor(1.0)
    TNT1 A 0 A_TakeInventory("PredZoom", 3)
    TNT1 A 0 A_PlaySound("/predator/pzoomout")
    Goto ZoomCooldown
  ZoomCooldown:
    TNT1 A 6
    TNT1 A 0 A_TakeInventory("PredZoomLatch", 1)
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "ReadyZoom")
    Goto Ready
  ZoomLatchHold:
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "ReadyZoom")
    Goto Ready
  ReadyZoom:
    PLPT A 1 A_WeaponReady(WRF_ALLOWZOOM)
    Loop
  Deselect:
    TNT1 A 0 A_TakeInventory("PredZoom", 3)
    TNT1 A 0 A_ZoomFactor(1.0)
    PLPT A 1 A_Lower
	NULL AAA 0 A_Lower
    Loop
  Select:
    PLPT A 1 A_Raise
	NULL AAA 0 A_Raise
    Loop
    Fire:
    TNT1 A 0 A_Jumpifinventory("Energy", 5, "PrimaryShot")
    TNT1 A 0 A_PlayWeaponSound("PCC")
    PLPT A 10 A_Print("Not enough energy.")
    Goto Ready
   PrimaryShot:	
    TNT1 A 0 A_TakeInventory("Energy", 5)
    TNT1 A 0 A_AlertMonsters
    TNT1 A 0 A_PlayWeaponSound("EMPSHOT")
    PLPT A 2 
    PLPT B 2 A_FireCustomMissile("EMPBall",0,3,4,-4)
    PLPT CDCB 2
    PLPT A 8
	PLPT A 1 A_ReFire
    Goto Ready
    AltFire:
    TNT1 A 0 A_Jumpifinventory("Energy", 5, "SecondaryShot")
    TNT1 A 0 A_PlayWeaponSound("PCC")
    PLPT A 10 A_Print("Not enough energy.")
    Goto Ready
   SecondaryShot:	
    TNT1 A 0 A_TakeInventory("Energy", 5)
    TNT1 A 0 A_AlertMonsters
    TNT1 A 0 A_PlayWeaponSound("EMPSHOT")
    PLPT A 2 
    PLPT B 2 A_FireCustomMissile("StunnerBall",0,3,4,-4)
    PLPT CDCB 2
    PLPT A 5 A_ReFire
	Goto Ready
  Spawn:
    PPIS A -1
    Stop
  }
}

ACTOR EMPBall
{
  Radius 7
  Height 10
  Speed 50
  Damage 5
  scale 0.5
  Decal Scorch
  Projectile
  Damagetype EMP
  +FORCERADIUSDMG
  +DEHEXPLOSION
  +RANDOMIZE
  DeathSound "EMPEXP"
  Obituary "Now that's was shoking!"
  States
  {
  Spawn:
    PLBA ABCD 2 Bright
    Loop
  Death:
    EMPE A 4
	EMPE B 4 bright A_Explode(10,128)
    EMPE C 4 bright
    EMPE D 4 bright
    stop
  }
}

ACTOR StunnerBall
{
  Radius 7
  Height 8
  Scale 0.5
  Speed 50
  Damage 3
  Projectile
  Damagetype Stunner
  +DEHEXPLOSION
  -NOGRAVITY
  Gravity 1.5
  Decal Scorch
  DeathSound "EMPEXP"
  Obituary "You fried."
  States
  {
  Spawn:
    PLBA ABCD 2 bright
    loop
  Death:
    EMPE A 0 bright A_ChangeFlag("NOGRAVITY",1)
    EMPE A 4
	EMPE B 4 bright A_Explode(5,128)
    EMPE C 4 bright
    EMPE D 4 bright
    stop
  }
}

