ACTOR ThrowingDaggers : Weapon
{
 Scale 0.3
  Inventory.RestrictedTo LightPredator
 +INVENTORY.RESTRICTABSOLUTELY
  Weapon.SlotNumber 3
  Weapon.AmmoUse 1
  Weapon.AmmoGive 2
  Weapon.AmmoType "Daggers"
  +Weapon.NOALERT
  +Weapon.Ammo_Optional
  Inventory.PickupSound "misc/p_pkup"
  Inventory.Pickupmessage "You got some Throwing Daggers!"
  Tag "Throwing Daggers"
  States
  {
  Ready:
	TNT1 A 0 A_JumpIfNoAmmo("NoDaggers")
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "ReadyZoom")
    PRTR A 1 A_WeaponReady(WRF_ALLOWZOOM)
    Loop
	NoDaggersLeft:
	TNT1 A 1 A_Print("You have no Daggers left.")
	Goto Ready
	NoDaggers:
	TNT1 A 1 A_WeaponReady(WRF_ALLOWZOOM)
	Goto Ready
  Zoom:
    TNT1 A 0 A_JumpIfInventory("PredZoomLatch", 1, "ZoomLatchHold")
    TNT1 A 0 A_GiveInventory("PredZoomLatch", 1)
    TNT1 A 0 A_JumpIfInventory("PredZoom", 3, "ZoomOut")
    TNT1 A 0 A_JumpIfInventory("PredZoom", 2, "Zoom3")
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "Zoom2")
  Zoom1:
    TNT1 A 1 A_ZoomFactor(2.0)
    TNT1 A 0 A_GiveInventory("PredZoom", 1)
    TNT1 A 0 A_PlaySound("/predator/pzoomin")
    Goto ZoomCooldown
  Zoom2:
    TNT1 A 1 A_ZoomFactor(4.0)
    TNT1 A 0 A_GiveInventory("PredZoom", 1)
    TNT1 A 0 A_PlaySound("/predator/pzoomin")
    Goto ZoomCooldown
  Zoom3:
    TNT1 A 1 A_ZoomFactor(8.0)
    TNT1 A 0 A_GiveInventory("PredZoom", 1)
    TNT1 A 0 A_PlaySound("/predator/pzoomin")
    Goto ZoomCooldown
  ZoomOut:
    TNT1 A 1 A_ZoomFactor(1.0)
    TNT1 A 0 A_TakeInventory("PredZoom", 3)
    TNT1 A 0 A_PlaySound("/predator/pzoomout")
    Goto ZoomCooldown
  ZoomCooldown:
    TNT1 A 6
    TNT1 A 0 A_TakeInventory("PredZoomLatch", 1)
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "ReadyZoom")
    Goto Ready
  ZoomLatchHold:
    TNT1 A 0 A_JumpIfInventory("PredZoom", 1, "ReadyZoom")
    Goto Ready
  ReadyZoom:
    PRTR A 1 A_WeaponReady(WRF_ALLOWZOOM)
    Loop
  Deselect:
	TNT1 A 0 A_JumpIfNoAmmo(2)
    TNT1 A 0 A_TakeInventory("PredZoom", 3)
    TNT1 A 0 A_ZoomFactor(1.0)
    PRTR A 1 A_Lower
    NULL AAA 0 A_Lower
	Loop
  Select:
    TNT1 A 0 A_JumpIfNoAmmo(2)
    PRTR A 1 A_Raise
	NULL AAA 0 A_Raise
    Loop
  Fire:
    TNT1 A 0 A_JumpIfNoAmmo("NoDaggersLeft")
    PRTN ABCBA 3
	TNT1 A 0 A_PlayWeaponSound("PRMELEE")
    PRTR B 1 A_FireCustomMissile("FlyingDagger",0,1,4,8)
    PRTR CDEFG 1
	TNT1 A 5 A_WeaponReady
	TNT1 A 0 A_JumpIfNoAmmo("NoDaggers")
    Goto Ready
  Spawn:
    DAGP A -1
    Stop
  }
}

ACTOR FlyingDagger
{
    Scale 0.4
	Speed 80
	Radius 2
	Height 2
	Damage 20
	Projectile
	DamageType Bleed
	PoisonDamage 15,10,0
	+NOEXTREMEDEATH
	+CANPUSHWALLS
	+ADDITIVEPOISONDAMAGE
    +ADDITIVEPOISONDURATION
	+ACTIVATEIMPACT
	+NOBLOCKMAP
	+BLOODSPLATTER  
	-NOGRAVITY
	AttackSound "FLESHHIT"
	Obituary "Now you got time to bleed."
	States
	{
	Spawn:
		TNT1 A 0 A_PlaySound("PRMELEE")
		DAGG ABCDEFG 2 
		Loop
	Death:
	    TNT1 A 0 A_ChangeFlag("NOGRAVITY", 1)
		DAGG GGG 1
		TNT1 A 0 A_SpawnItemEx("Daggers", 0, 0, 0, 0, 0, 3, 0)
		Stop
	}
}

